<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Dialog id="registerDialog" title="Register New User" draggable="true" resizable="true">
    <VBox id="_IDGenVBox26" class="sapUiSmallMargin">
      <Input id="regUsername" placeholder="Username" />
      <Input id="regEmail" placeholder="Email" type="Email" />
      <Input id="regPassword" placeholder="Password" type="Password" />
    </VBox>
    <beginButton>
      <Button id="_IDGenButton16" text="Register" press="onRegisterSubmit" />
    </beginButton>
    <endButton>
      <Button id="_IDGenButton17" text="Cancel" press="onRegisterCancel" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>

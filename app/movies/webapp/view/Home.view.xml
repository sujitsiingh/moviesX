<mvc:View
    controllerName="movies.controller.Login"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:table="sap.ui.table"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:unified="sap.ui.unified"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:commons="sap.suite.ui.commons"
    xmlns:l="sap.ui.layout"
>
    <Page
        id="page"
        title="MoviesX"
    >
        <!-- tool header of the page -->
        <tnt:ToolHeader
            id="shellLikeToolHeader"
            class="sapUiTinyMargin"
        >
            <Button
                id="_IDGenButton3"
                icon="sap-icon://menu2"
                type="Transparent"
                tooltip="Menu"
                press=".onCollapseExpandPress"
            >
                <layoutData>
                    <OverflowToolbarLayoutData
                        id="_IDGenOverflowToolbarLayoutData"
                        priority="NeverOverflow"
                    />
                </layoutData>
            </Button>
            <Title
                text="MoviesX"
                wrapping="false"
                id="productName"
            >
                <layoutData>
                    <OverflowToolbarLayoutData
                        id="_IDGenOverflowToolbarLayoutData2"
                        priority="Disappear"
                    />
                </layoutData>
            </Title>
            <ToolbarSpacer id="_IDGenToolbarSpacer1" />

            <!-- menu items for theme -->
            <MenuButton
                id="_IDGenMenuButton"
                text="Theme"
                type="Transparent"
                buttonMode="Split"
            >
                <menu>
                    <Menu
                        id="_IDGenMenu"
                        itemSelected=".onMenuItem"
                    >
                        <items>
                            <MenuItem
                                id="_IDGenMenuItem6"
                                text="Morning Horizon"
                            >
                                <customData>
                                    <core:CustomData
                                        id="_IDGenCustomData1"
                                        key="action"
                                        value="onThemeMH"
                                    />
                                </customData>
                            </MenuItem>
                            <MenuItem
                                id="_IDGenMenuItem7"
                                text="Evening Horizon (dark)"
                            >
                                <customData>
                                    <core:CustomData
                                        id="_IDGenCustomData2"
                                        key="action1"
                                        value="onThemeEve"
                                    />
                                </customData>
                            </MenuItem>
                            <MenuItem
                                id="_IDGenMenuItem3"
                                text="Quartz Light"
                            >
                                <customData>
                                    <core:CustomData
                                        id="_IDGenCustomData3"
                                        key="action2"
                                        value="onThemeQL"
                                    />
                                </customData>
                            </MenuItem>
                            <MenuItem
                                id="_IDGenMenuItem4"
                                text="Quartz Dark"
                            >
                                <customData>
                                    <core:CustomData
                                        id="_IDGenCustomData4"
                                        key="action3"
                                        value="onThemeQD"
                                    />
                                </customData>
                            </MenuItem>
                        </items>
                    </Menu>
                </menu>
            </MenuButton>

            <tnt:ToolHeaderUtilitySeparator
                id="_IDGenToolHeaderUtilitySeparator"
            />
            <VBox
                id="profileDropdown"
                class="sapUiSmallMargin"
            >
                <Button
                    id="profile_button"
                    text="Welcome, {userModel>/username}"
                    press=".profile_dropdown"
                />
            </VBox>
        </tnt:ToolHeader>

        <!-- side navigation -->
        <HBox id="_IDGenHBox3">
            <!-- side bar -->
            <VBox id="_IDGenVBox7">
                <tnt:SideNavigation
                    id="sideNavigation"
                    selectedKey="walked"
                    expanded="false"
                >
                    <tnt:NavigationList id="_IDGenNavigationList">
                        <tnt:NavigationListItem
                            id="_IDGenNavigationListItem1"
                            text="Home"
                            icon="sap-icon://home"
                            press=".onHomePress"
                        />
                        <tnt:NavigationListGroup
                            id="_IDGenNavigationListGroup"
                            text="New"
                        >
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem2"
                                text="Add New Movie"
                                icon="sap-icon://people-connected"
                                press=".onAddMoviesPress"
                            />
                        </tnt:NavigationListGroup>
                        <tnt:NavigationListGroup
                            id="_IDGenNavigationListGroup1"
                            text="Recently used"
                        >
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem8"
                                text="View Movies"
                                icon="sap-icon://kpi-managing-my-area"
                                press=".onViewMoviesPress"
                            />
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem9"
                                text="Reviews"
                                icon="sap-icon://present"
                                key="reviews"
                                press=".onReviewsPress"
                            />
                        </tnt:NavigationListGroup>
                    </tnt:NavigationList>
                    <tnt:fixedItem>
                        <tnt:NavigationList id="_IDGenNavigationList1">
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem14"
                                text="Movie Slate"
                                icon="sap-icon://tri-state"
                                press=".onViewMovieSlatePress"
                            />
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem15"
                                selectable="false"
                                href="https://www.imdb.com/chart/top/"
                                target="_blank"
                                text="IMDB Page"
                                icon="sap-icon://attachment"
                            />
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem16"
                                selectable="false"
                                href="https://sap.com"
                                target="_top"
                                text="External Link _top"
                                icon="sap-icon://attachment"
                            />
                            <tnt:NavigationListItem
                                id="_IDGenNavigationListItem17"
                                text="Compare"
                                icon="sap-icon://compare"
                            />
                        </tnt:NavigationList>
                    </tnt:fixedItem>
                </tnt:SideNavigation>
            </VBox>

            <VBox id="_IDGenVBox8">
                <!-- Panel 1 -->
                <Panel
                    id="panel1"
                    width="50rem"
                    class="sapUiResponsiveMargin"
                    accessibleRole="Region"
                    visible="false"
                >
                    <headerToolbar>
                        <OverflowToolbar id="_IDGenOverflowToolbar">
                            <Title
                                id="_IDGenTitle"
                                text="Add New Movies.."
                            />
                        </OverflowToolbar>
                    </headerToolbar>
                    <Breadcrumbs
                        id="_IDGenBreadcrumbs"
                        currentLocationText="Add new item"
                    >
                        <Link
                            id="_IDGenLink"
                            text="Master"
                        />
                    </Breadcrumbs>

                    <HBox id="_IDGenHBox">
                        <VBox id="_IDGenVBox1">
                            <Label
                                id="_IDGenLabel1"
                                text="title"
                            />
                            <Input
                                id="_IDGenInput1"
                                placeholder="Enter Title of the movie"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox2">
                            <Label
                                id="_IDGenLabel2"
                                text="overview"
                            />
                            <Input
                                id="_IDGenInput2"
                                placeholder="Enter overview of the movie"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox3">
                            <Label
                                id="_IDGenLabel3"
                                text="releaseYear"
                            />
                            <Input
                                id="_IDGenInput3"
                                placeholder="Enter release year of the movie"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox1">
                        <VBox id="_IDGenVBox4">
                            <Label
                                id="_IDGenLabel4"
                                text="runtimeMin"
                            />
                            <Input
                                id="_IDGenInput4"
                                placeholder="Enter run time(mins)"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox5">
                            <Label
                                id="_IDGenLabel5"
                                text="Reviews"
                            />
                            <Input
                                id="_IDGenInput5"
                                placeholder="Enter reviews"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox6">
                            <Label
                                id="_IDGenLabel6"
                                text="casting"
                            />
                            <Input
                                id="_IDGenInput6"
                                placeholder="Enter cast name"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox2">
                        <VBox id="_IDGenVBox">
                            <Label
                                id="_IDGenLabel7"
                                text="currency"
                            />
                            <Input
                                id="_IDGenInput7"
                                value="{currency}"
                                showValueHelp="true"
                                valueHelpRequest=".onCurrencyVH"
                                maxLength="3"
                                placeholder="Select currency"
                            />
                        </VBox>
                    </HBox>
                    <HBox id="_IDGenHBox5">
                        <VBox id="_IDGenVBox9">
                            <Button
                                id="_IDGenButton1"
                                text="Add Me"
                                press=".onAddMe"
                            />
                        </VBox>
                    </HBox>
                </Panel>

                <!-- Panel 2 -->
                <Panel
                    id="panel2"
                    headerText="All Moviews View"
                    visible="false"
                    class="sapUiResponsiveMargin"
                    width="100%"
                >
                    <!-- <Label id="_IDGenLabel" text="Enter the details of the movie" /> -->
                    <Table
                        id="moviesTable"
                        items="{path: '/Movies', parameters: {
                            $count: false,
                            $$groupId: '$direct'
                        }}"
                    >
                        <headerToolbar>
                            <OverflowToolbar id="_IDGenOverflowToolbar2">
                                <content>
                                    <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                                    <SearchField
                                        id="searchField"
                                        width="20%"
                                        placeholder="Search this table"
                                        search=".onSearchMovie"
                                    />
                                    <Button
                                        id="refreshUsersButton"
                                        icon="sap-icon://refresh"
                                        tooltip="Refresh"
                                        press="onRefresh('moviesTable')"
                                    />
                                    <Button
                                        id="_IDGenButton10"
                                        icon="sap-icon://excel-attachment"
                                        press=".onExport"
                                    />
                                    <Button
                                        id="deleteUserButton"
                                        icon="sap-icon://delete"
                                        tooltip="Delete Row"
                                        press=".onDelete"
                                    >
                                        <layoutData>
                                            <OverflowToolbarLayoutData
                                                id="_IDGenOverflowToolbarLayoutData1"
                                                priority="NeverOverflow"
                                            />
                                        </layoutData>
                                    </Button>
                                </content>
                            </OverflowToolbar>
                        </headerToolbar>
                        <columns>
                            <Column id="_IDGenColumn">
                                <Text
                                    id="_IDGenText1"
                                    text="Title"
                                />
                            </Column>
                            <Column id="_IDGenColumn1">
                                <Text
                                    id="_IDGenText2"
                                    text="Overview"
                                />
                            </Column>
                            <Column id="_IDGenColumn2">
                                <Text
                                    id="_IDGenText3"
                                    text="Release Year"
                                />
                            </Column>
                            <Column id="_IDGenColumn3">
                                <Text
                                    id="_IDGenText4"
                                    text="Runtime (min)"
                                />
                            </Column>
                            <Column id="_IDGenColumn4">
                                <Text
                                    id="_IDGenText5"
                                    text="Currency"
                                />
                            </Column>
                            <Column id="_IDGenColumn5">
                                <Text
                                    id="_IDGenText6"
                                    text="Reviews"
                                />
                            </Column>
                            <!-- <Column id="_IDGenColumn6">
                                <Text id="_IDGenText7"  text="Cast"/>
                            </Column> -->
                            <Column id="_IDGenColumn7">
                                <Text
                                    id="actionText"
                                    text="Action"
                                />
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem id="_IDGenColumnListItem">
                                <cells>
                                    <Text
                                        id="_IDGenText8"
                                        text="{title}"
                                    />
                                    <Text
                                        id="_IDGenText9"
                                        text="{overview}"
                                    />
                                    <Text
                                        id="_IDGenText10"
                                        text="{releaseYear}"
                                    />
                                    <Text
                                        id="_IDGenText11"
                                        text="{runtimeMin}"
                                    />
                                    <Text
                                        id="_IDGenText12"
                                        text="{currency_code}"
                                    />
                                    <Text
                                        id="_IDGenText13"
                                        text="{numReviews}"
                                    />
                                    <!-- <Text id="_IDGenText12" text="{= ${cast} ? ${cast}.join(', ') : ''}"/> -->
                                    <!-- <Text id="_IDGenText14" text="{ path: 'cast', formatter: '.formatCast' }"/> -->
                                    <!-- <VBox id="_IDGenVBox10" items="{path: 'cast'}"><items><Text id="_IDGenText" text="{.}"/></items></VBox> -->

                                    <!-- <HBox id="_IDGenHBox4" class="commaSeparated" items="{path: 'cast'}">
                                        <items>
                                            <Text id="_IDGenText14" text="{.}" />
                                        </items>
                                    </HBox> -->
                                    <Button
                                        id="_IDGenButton2"
                                        icon="sap-icon://action"
                                        text="Click Me"
                                        press="onActionPress($event)"
                                    />
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </Panel>

                <!-- Panel 3 EDITING PANEL-->
                <Panel
                    id="panel3"
                    width="50rem"
                    class="sapUiResponsiveMargin"
                    accessibleRole="Region"
                    visible="false"
                >
                    <headerToolbar>
                        <OverflowToolbar id="_IDGenOverflowToolbar3">
                            <Title
                                id="_IDGenTitle3"
                                text="Movies.."
                            />
                        </OverflowToolbar>
                    </headerToolbar>
                    <Breadcrumbs
                        id="_IDGenBreadcrumbs3"
                        currentLocationText="Edit the Entries.."
                    >
                        <Link
                            id="_IDGenLink3"
                            text="Home"
                        />
                    </Breadcrumbs>

                    <HBox id="_IDGenHBoxP3">
                        <VBox id="itemCodeBox">
                            <Label
                                id="icLabel"
                                text="id"
                            />
                            <Input
                                id="itemCode"
                                placeholder="ID of the movie"
                                editable="false"
                            />
                        </VBox>
                        <VBox id="_IDGenVBoxP3">
                            <Label
                                id="_IDGenLabelP3"
                                text="title"
                            />
                            <Input
                                id="editTitle"
                                placeholder="Title of the movie"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox11">
                            <Label
                                id="_IDGenLabelP03"
                                text="overview"
                            />
                            <Input
                                id="editOverview"
                                placeholder="Enter Overview of the movie"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox12">
                            <Label
                                id="_IDGenLabel"
                                text="releaseYear"
                            />
                            <Input
                                id="editReleaseYear"
                                placeholder="Enter release year of the movie"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox4">
                        <VBox id="_IDGenVBox13">
                            <Label
                                id="_IDGenLabel8"
                                text="runtimeMin"
                            />
                            <Input
                                id="editRunTime"
                                placeholder="Enter run time(mins)"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox14">
                            <Label
                                id="_IDGenLabel9"
                                text="Reviews"
                            />
                            <Input
                                id="editReviews"
                                placeholder="Enter reviews"
                            />
                        </VBox>
                        <VBox id="_IDGenVBox15">
                            <Label
                                id="_IDGenLabel10"
                                text="casting"
                            />
                            <Input
                                id="editCast"
                                placeholder="Enter cast name"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox6">
                        <VBox id="_IDGenVBox16">
                            <Label
                                id="_IDGenLabel11"
                                text="currency"
                            />
                            <Input
                                id="editCurrency"
                                placeholder="Enter currency"
                            />
                        </VBox>
                    </HBox>
                    <HBox id="_IDGenHBox7">
                        <VBox id="_IDGenVBox17">
                            <Button
                                id="_IDGenButton8"
                                text="Edit"
                                press=".onEditMe"
                            />
                        </VBox>
                    </HBox>
                </Panel>

                <!-- panel 4 CHARTS PANEL-->
                <Panel
                    id="panel4"
                    visible="false"
                    headerText="MoviesX Charts"
                    expandable="false"
                    width="55rem"
                    class="sapUiResponsiveMargin"
                >
                    <VBox id="_IDGenVBox18">
                        <Title
                            id="_IDGenTitle1"
                            text="MoviesX Top { path: '/Movies/$count' } movies"
                            level="H1"
                        />
                        <Text
                            id="_IDGenText7"
                            text="As rated by regular MoviesX voters."
                        />
                    </VBox>

                    <VBox id="_IDGenVBox24">
                        <Text
                            id="_IDGenText14"
                            text="0 of { path: '/Movies/$count' } WATCHED"
                        />
                        <ProgressIndicator
                            id="_IDGenProgressIndicator"
                            percentValue="0"
                            displayValue="0%"
                            showValue="true"
                        />
                        <Button
                            id="_IDGenButton"
                            text="Show Progress"
                            icon="sap-icon://status-positive"
                            press="onShowProgress"
                        />
                    </VBox>

                    <!-- <content>
                        <VBox
                            id="_IDGenVBox23"
                            class="gridVbox"
                        >
                            <List
                                id="moviesXList"
                                items="{
                                path: '/Movies',
                                parameters: { 
                                    $expand: {
                                        reviews: {
                                            $select: ['ID','movie_ID','rating']
                                        }
                                    },
                                    $select: ['ID','title','overview','releaseYear', 'runtimeMin','currency_code','numReviews']
                                }
                            }"
                            >
                                <ObjectListItem
                                    id="_IDGenObjectListItem3"
                                    title="{title}"
                                    numberUnit="★"
                                    press="onListItemPress"
                                >
                                    <attributes>
                                        <ObjectAttribute
                                            id="_IDGenObjectAttribute3"
                                            text="Release Year: {releaseYear}"
                                        />
                                        <ObjectAttribute
                                            id="_IDGenObjectAttribute4"
                                            text="{runtimeMin} minutes Run time"
                                        />
                                    </attributes>
                                </ObjectListItem>
                            </List>
                        </VBox>
                    </content> -->
                    <Table
                        id="moviesxChart"
                        inset="false"
                        items="{
			path: '/Movies',
            parameters: { 
                $expand: {
                reviews: {
                    $select: ['ID','movie_ID','rating']
                }
                },
                $select: ['ID','title','overview','releaseYear', 'runtimeMin','currency_code','numReviews']
            }
		}"
            >
                        <columns>
                            <Column
                                id="_IDGenColumn6"
                                width="14em"
                            >
                                <Text
                                    id="_IDGenText20"
                                    text="Movies Name"
                                />
                                
                            </Column>
                            <Column
                                id="_IDGenColumn8"
                                minScreenWidth="Tablet"
                                demandPopin="true"
                            >
                                <Text
                                    id="_IDGenText21"
                                    text="Release Year"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn9"
                                minScreenWidth="Tablet"
                                demandPopin="true"
                                hAlign="End"
                            >
                                <Text
                                    id="_IDGenText22"
                                    text="Run time (min)"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn11"
                                hAlign="End"
                            >
                                <Text
                                    id="_IDGenText24"
                                    text="Rating"
                                />
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem
                                id="_IDGenColumnListItem1"
                                vAlign="Middle"
                            >
                                <cells>
                                    <ObjectIdentifier
                                        id="_IDGenObjectIdentifier"
                                        title="{title}"
                                        text="{}"
                                    />
                                    <Text
                                        id="_IDGenText25"
                                        text="{releaseYear}"
                                    />
                                    <Text
                                        id="_IDGenText26"
                                        text="{runtimeMin}"
                                    />
                                    <Button id="i-btn" text="" type="Neutral" press="onIbtnPress"></Button>
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </Panel>

                <!-- Panel 5 HOME PANEL-->
                <Panel
                    id="panel5"
                    visible="true"
                    width="55rem"
                    class="sapUiResponsiveMargin"
                    accessibleRole="Region"
                >
                    <content>
                        <VBox
                            id="_IDGenHBox14"
                            width="100%"
                        >
                            <f:GridList id="gridList">
                                <f:GridListItem id="_IDGenGridListItem6">
                                    <VBox
                                        height="100%"
                                        justifyContent="SpaceBetween"
                                        id="_IDGenVBox20"
                                        class="sapUiSmallMargin"
                                    >
                                        <layoutData>
                                            <FlexItemData
                                                id="_IDGenFlexItemData"
                                                growFactor="1"
                                                shrinkFactor="0"
                                            />
                                        </layoutData>
                                        <VBox
                                            id="_IDGenVBox21"
                                            class="sapUiSmallMargin"
                                        >
                                            <Title
                                                id="_IDGenTitle2"
                                                text="Movies"
                                                wrapping="true"
                                            />
                                            <Label
                                                id="_IDGenLabel19"
                                                text="{/Movies/$count}"
                                                wrapping="true"
                                            />
                                        </VBox>
                                    </VBox>
                                </f:GridListItem>
                                <f:GridListItem id="_IDGenGridListIt2">
                                    <VBox
                                        height="100%"
                                        justifyContent="SpaceBetween"
                                        id="_IDGeBox4"
                                        class="sapUiSmallMargin"
                                    >
                                        <layoutData>
                                            <FlexItemData
                                                id="_IDGenFlexemData2"
                                                growFactor="1"
                                                shrinkFactor="0"
                                            />
                                        </layoutData>

                                        <VBox
                                            id="_IDGenVBox22"
                                            class="sapUiSmallMargin"
                                        >
                                            <Title
                                                id="_IDGentle4"
                                                text="Genres"
                                                wrapping="true"
                                            />
                                            <Label
                                                id="_IDGenLel2"
                                                text="{ path: '/Genres/$count' }"
                                                wrapping="true"
                                            />
                                        </VBox>
                                    </VBox>
                                </f:GridListItem>
                            </f:GridList>
                        </VBox>
                        <VBox id="_IDGenHBox15">
                            <f:GridList id="_IDGenGridList2">
                                <f:GridListItem id="_IDGenGridListItem4">
                                    <commons:ChartContainer
                                        id="chartContainer"
                                        showFullScreen="true"
                                        showPersonalization="false"
                                        personalizationPress="attachPersonalizationPress"
                                        contentChange="attachContentChange"
                                        title="Rating TO Movies"
                                    >
                                        <commons:content>
                                            <commons:ChartContainerContent
                                                id="_IDGenChartContainerContent"
                                                icon="sap-icon://line-chart"
                                                title="Chart"
                                            >
                                                <commons:content>
                                                    <viz:VizFrame
                                                        id="chartContainerVizFrame"
                                                        width="100%"
                                                        uiConfig="{applicationSet:'fiori'}"
                                                        vizType="line"
                                                    >
                                                        <viz:dataset>
                                                            <viz.data:FlattenedDataset
                                                                id="_IDGenFlattenedDataset"
                                                                data="{/Movies}"
                                                            >
                                                                <viz.data:dimensions
                                                                >
                                                                    <viz.data:DimensionDefinition
                                                                        id="_IDGenDimensionDefinition"
                                                                        name="MovieName"
                                                                        value="{title}"
                                                                    />
                                                                </viz.data:dimensions>
                                                                <viz.data:measures
                                                                >
                                                                    <viz.data:MeasureDefinition
                                                                        id="_IDGenMeasureDefinition1"
                                                                        name="Reviews"
                                                                        value="{numReviews}"
                                                                    />
                                                                </viz.data:measures>
                                                            </viz.data:FlattenedDataset>
                                                        </viz:dataset>
                                                        <viz:feeds>
                                                            <viz.feeds:FeedItem
                                                                id="valueAxisFeed"
                                                                uid="valueAxis"
                                                                type="Measure"
                                                                values="Reviews"
                                                            />
                                                            <viz.feeds:FeedItem
                                                                id="categoryAxisFeed"
                                                                uid="categoryAxis"
                                                                type="Dimension"
                                                                values="MovieName"
                                                            />
                                                        </viz:feeds>
                                                    </viz:VizFrame>
                                                </commons:content>
                                            </commons:ChartContainerContent>
                                        </commons:content>
                                    </commons:ChartContainer>
                                </f:GridListItem>
                                <!-- <f:GridListItem id="_IDGenGridListItem5" /> -->
                            </f:GridList>
                        </VBox>
                    </content>
                </Panel>

                <!-- panel 6 REVIEWS PANEL-->
                <Panel
                    id="panel6"
                    width="55rem"
                    class="sapUiResponsiveMargin"
                    accessibleRole="Region"
                    visible="false"
                >
                    <content>
                        <List
                            id="reviewsList"
                            headerText="All Reviews"
                            mode="None"
                            items="{
                                path: '/Reviews',
                                parameters: { 
                                    $expand: {
                                        movie: {
                                            $select: ['ID','title']
                                        }
                                    },
                                    $select: ['ID','name','rating','comment', 'movie_ID','user_ID']
                                }
                            }"
                        >
                            <ObjectListItem
                                id="_IDGenObjectListItem1"
                                title="{name}"
                                number="{path: 'rating'}"
                                numberUnit="★"
                                intro="{= ${movie/title} ? 'Movie: ' + ${movie/title} : '' }"
                            >
                                <!-- <firstStatus>
                                    <ObjectStatus
                                        id="_IDGenObjectStatus5"
                                        text="Updated: {path: 'modifiedAt', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' }}"
                                    />
                                </firstStatus> -->

                                <attributes>
                                    <ObjectAttribute
                                        id="_IDGenObjectAttribute1"
                                        title="Comment"
                                        text="{ path: 'comment', formatter: '._formatCommentAttr' }"
                                    />
                                </attributes>
                            </ObjectListItem>
                        </List>
                    </content>
                </Panel>

                <!-- Panel 7 User REVIEW PAGE-->
                <Panel
                    id="panel7"
                    width="auto"
                    class="sapUiResponsiveMargin"
                    accessibleRole="Region"
                    visible="false"
                    headerText="User Reviews"
                >
                    <content>
                        <HBox
                            id="_IDGenHBox17"
                            justifyContent="End"
                        >
                            <Button
                                id="_IDGenButton9"
                                icon="sap-icon://add"
                                text="Add"
                                type="Emphasized"
                                press=".onAddPress"
                            />
                        </HBox>

                        <List
                            id="reviewsList1"
                            mode="None"
                            items="{
              path: '/Reviews',
              parameters: { 
                $expand: {
                  movie: {
                    $select: ['ID','title']
                  }
                },
                $select: ['ID','name','rating','comment', 'movie_ID','user_ID']
              }
            }"
                        >
                            <ObjectListItem
                                id="_IDGenObjectListItem2"
                                title="{name}"
                                number="{path: 'rating'}"
                                numberUnit="★"
                                intro="{= ${movie/title} ? 'Movie: ' + ${movie/title} : '' }"
                            >
                                <!-- <firstStatus>
                                    <ObjectStatus
                                        id="_IDGenObjectStatus5"
                                        text="Updated: {path: 'modifiedAt', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' }}"
                                    />
                                </firstStatus> -->

                                <attributes>
                                    <ObjectAttribute
                                        id="_IDGenObjectAttribute2"
                                        title="Comment"
                                        text="{ path: 'comment', formatter: '._formatCommentAttr' }"
                                    />
                                </attributes>
                            </ObjectListItem>
                        </List>
                    </content>
                </Panel>
            </VBox>
        </HBox>

